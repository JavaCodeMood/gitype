<?xml version="1.0" encoding="utf-8" ?>
<logs>
    <info prefix="[INFO]" flag="">
        <console output="stderr" foreground="green" />
    </info>
 
    <debug prefix="[DEBUG]">
        <console output="stderr" foreground="yellow" />
    </debug>
 
    <trace prefix="[TRACE]">
        <console output="stderr" foreground="yellow" />
    </trace>
 
    <warn prefix="[WARN]">
        <console output="stderr" foreground="yellow" />
    </warn>
 
    <error prefix="[ERROR]" flag="log.llongfile">
        <console output="stderr" foreground="red" />
    </error>
 
    <critical prefix="[CRITICAL]" flag="log.llongfile">
        <console output="stderr" foreground="red" />
    </critical>
</logs>
